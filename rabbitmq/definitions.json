{
  "rabbit_version": "3.13.0",
  "rabbitmq_version": "3.13.0",
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "quiz_questions",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "dlx_exchange",
        "x-dead-letter-routing-key": "failed_questions"
      }
    },
    {
      "name": "gemini_responses",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "dlx_exchange",
        "x-dead-letter-routing-key": "failed_responses"
      }
    },
    {
      "name": "failed_questions_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "failed_responses_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "dlx_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "dlx_exchange",
      "vhost": "/",
      "destination": "failed_questions_queue",
      "destination_type": "queue",
      "routing_key": "failed_questions",
      "arguments": {}
    },
    {
      "source": "dlx_exchange",
      "vhost": "/",
      "destination": "failed_responses_queue",
      "destination_type": "queue",
      "routing_key": "failed_responses",
      "arguments": {}
    }
  ]
}